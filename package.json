    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^4.4.1",
    "eslint-plugin-flow": "^2.29.1",
    "eslint-plugin-react": "^7.2.0",
    "flow-bin": "^0.52.0",
    "flow-copy-source": "^1.2.0",
    "jest-cli": "^20.0.4",
